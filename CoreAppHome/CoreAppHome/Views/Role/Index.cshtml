@model  IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>

<h1>Role List</h1>
<a asp-action="Create">Create Role</a>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <td>Id</td>
            <td>Name</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var role in Model)
        {
            <tr>
                <td>@role.Id</td>
                <td>@role.Name</td>
            </tr>
        }

    </tbody>
</table>